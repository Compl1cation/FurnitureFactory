@model IEnumerable<FurnitureFactory.Models.Furniture>

@{
    ViewBag.Title = "Index";
}

<h2>Browse Furniture</h2>


<form method="get" action="@Url.Action("Index")"
data-prods-ajax="true" data-prods-target="#productList">

    <input type="search" class="searchBox" name="searchTerm" data-autocomplete-source="@Url.Action("Autocomplete", "BrowseFurniture")" />
    <input type="submit" class="button" value="Search by Name" />    
</form>


@Html.Partial("_Products", Model)



