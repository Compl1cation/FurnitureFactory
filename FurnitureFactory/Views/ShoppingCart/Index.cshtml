@using FurnitureFactory.Models.ViewModels
@model IEnumerable<ProductQuantityViewModel>

@{
    ViewBag.Title = "Shopping Cart";
}

<div class="content">
<h2>Shopping Cart</h2>
    <table class="cart table">
        <tr>
            <th>
                Furniture
            </th>
            <th>
                Quantity
            </th>
            <th>
                Price
            </th>
        </tr>
  </table>
@Html.ActionLink("Back to Furniture","Index", "BrowseFurniture")

<button type="button" name="PostOrder" class="postButton">Send Order</button>
</div>