@model IEnumerable<FurnitureFactory.Models.Furniture>

@{
    ViewBag.Title = "Index";
}

<h2 class="whiteTrebouchet">Furniture Manager</h2>

<form method="post" action="@Url.Action("Index")"
      data-prods-ajax="true" data-prods-target="#productList">

    <input type="search" class="searchBox" name="searchTerm" data-autocomplete-source="@Url.Action("Autocomplete", "FurnitureManager")" />
    <input type="submit" class="button" value="Search by Name" />
</form>

@Html.Partial("_ManageFurniture", Model)

